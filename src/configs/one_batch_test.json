{
  "name": "one_batch_test",
  "n_gpu": 1,
  "preprocessing": {
    "sr": 16000,
    "spectrogram": {
      "type": "MelSpectrogram",
      "args": {
      }
    },
    "log_spec": true
  },
  "arch": {
    "type": "RawNet2",
    "args": {
    }
  },
  "data": {
    "train": {
      "batch_size": 10,
      "num_workers": 0,
      "datasets": [
        {
          "type": "ASVSpoofDataset",
          "args": {
            "part": "LA", 
            "split": "train"
          }
        }
      ]
    },
    "dev": {
      "batch_size": 10,
      "num_workers": 0,
      "datasets": [
        {
          "type": "ASVSpoofDataset",
          "args": {
            "part": "LA", 
            "split": "dev"
          }
        }
      ]
    },
    "eval": {
      "batch_size": 10,
      "num_workers": 0,
      "datasets": [
        {
          "type": "ASVSpoofDataset",
          "args": {
            "part": "LA", 
            "split": "eval"
          }
        }
      ]
    }
  },
  "optimizer": {
    "type": "SGD",
    "args": {
      "lr": 1e-2
    }
  },
  "loss": {
    "type": "CrossEntropyLoss",
    "args": {
    }
  },
  "metrics": [
    {
      "type": "EER",
      "args": {
        "name": "EER"
      }
    }
  ],
  "lr_scheduler": {
    "type": "ExponentialLR",
    "args": {
      "gamma": 1
    }
  },
  "trainer": {
    "epochs": 50,
    "save_dir": "saved/",
    "save_period": 5,
    "verbosity": 2,
    "monitor": "min val_loss",
    "early_stop": 100,
    "visualize": "wandb",
    "wandb_project": "antispoofing",
    "len_epoch": 100,
    "grad_norm_clip": 10
  }
}
